{
  "name": "node-expresss-postgres-react-starter",
  "version": "0.4.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-airbnb": "^2.5.1",
    "babel-runtime": "6.26.0",
    "bootstrap": "^4.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "concurrently": "^3.6.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "express": "^4.16.3",
    "express-promise-router": "^3.0.2",
    "extract-text-webpack-plugin": "3.0.2",
    "faker": "^4.1.0",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "http-errors": "^1.6.3",
    "jest": "20.0.4",
    "massive": "^5.0.0",
    "morgan": "^1.9.0",
    "node-pg-migrate": "^3.5.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.2",
    "raf": "3.4.0",
    "react": "^16.4.1",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.1",
    "resolve": "1.6.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "client": "node scripts/start-client.js",
    "server": "node scripts/start-server.js",
    "start": "node scripts/start-server-production.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "node scripts/build.js && npm run lint",
    "lint": "eslint server/**/*.js",
    "pg-migrate": "node-pg-migrate",
    "migrate": "node scripts/db-migrate",
    "seed": "npm run migrate && node scripts/db-seed.js",
    "test-client": "node scripts/test-client.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "client-src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/client-src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/client-src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "airbnb"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "comma-dangle": "off",
      "no-console": "off",
      "jsx-a11y/label-has-for": [
        "error",
        {
          "required": "some"
        }
      ],
      "react/prefer-stateless-function": "warn",
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ]
    }
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:5000/",
      "ws": false
    }
  }
}
